{"name":"Pi","type":"character","img":"systems/vermine2047/assets/images/avatars/Vermine-Pi.png","system":{"minorWound":{"threshold":1,"value":3,"min":0,"max":4},"majorWound":{"threshold":4,"value":"0","min":0,"max":3},"deadlyWound":{"threshold":8,"value":null,"min":0,"max":1},"identity":{"height":0,"weight":0,"totem":"builder","age":12,"ageType":"1","profile":"Virtuose","theme":"Petit génie","instincts":"Toujours trouver une solution originale","prohibits":"Ne jamais détruire un vestige du passé","objectives":"à définir avec le groupe\n                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                 