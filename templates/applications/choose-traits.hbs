<div class="trait choose grid grid-2col" style="padding:5px;">
    {{#each traits as |trait ind|}}
    <div class="form-group" data-trait="{{ind}}">
        <label>
            <input type="checkbox" class="trait-selector"
                data-trait="{{ind}}" />
            {{trait.name}}
        </label>
        <p class="hint">{{trait.description}}</p>
    </div>
    {{/each}}

</div>